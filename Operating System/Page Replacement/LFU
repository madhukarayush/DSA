#include<bits/stdc++.h>
#define Max 1000
using namespace std;


int main(){
    int n,m;
    cout<<"Enter the number of processes: ";
    cin>>n;
    cout<<"Enter the number of frames: ";
    cin>>m;
    int *a=new int[n];
    cout<<"Enter the page reference string: ";
    for(int i=0;i<n;i++){
   	 cin>>a[i];
    }
    int *fifo=new int[n]();
    int pf=0;
    int *ct=new int[Max]();
    int *check=new int[Max]();
    int nof=0;
    for(int i=0;i<n;i++){
   	 if(check[a[i]]==1){
   		 ct[a[i]]+=1;
   	 }
   	 else if(nof<m){
   		 fifo[i]=a[i];
   		 check[a[i]]=1;
   		 ct[a[i]]=1;
   		 nof++;
   	 }
   	 else{
   		 int Min=Max;
   		 int ind=-1;
   		 for(int j=0;j<n;j++){
   			 if(fifo[j]!=0&&fifo[j]!=-1&&ct[fifo[j]]<Min){
   				 Min=ct[fifo[j]];
   				 ind=j;
   			 }
   		 }
   		 int p=fifo[ind];
   		 ct[p]=0;
   		 check[p]=0;
   		 fifo[ind]=-1;
   		 pf++;
   		 fifo[i]=a[i];
   		 check[a[i]]=1;
   		 ct[a[i]]=1;
   	 }
    }
    cout<<"\nPage fault occurs "<<(pf+m)<<" number of times"<<endl;
    delete[] fifo;
    delete[] a;
    return 0;
}



